{
    "name": "vue-brazil",
    "version": "0.2.0",
    "main": "src/index.js",
    "scripts": {
        "dev": "node_modules/webpack/bin/webpack.js --progress --config webpack.config.js",
        "installall": "npm i && cd sampleapp && npm i ",
        "start": "cd sampleapp && npm run start",
        "watch": "npm run dev & onchange 'src/**/*.js' -- npm run dev",
        "watch:test": "onchange 'src/**/*.ts' '*.ts' -- npm run test",
        "dist": "npm run dev && cp -R src/* dist/",
        "test": "cd sampleapp && npm run test",
        "build-docs": "node build/build-docs.js",
        "build-main": "node build/build-main.js",
        "init-sample": "vue init webpack sampleapp",
        "publishnpm": "npm run dist && npm publish"
    },
    "dependencies": {
        "js-brasil": "^2.5.3",
        "vue": "^2.5.13"
    },
    "devDependencies": {
        "babel-core": "^6.22.1",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-plugin-transform-vue-jsx": "^3.5.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-stage-2": "^6.22.0",
        "chalk": "^2.0.1",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^1.1.4",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-webpack-plugin": "^2.30.1",
        "node-notifier": ">=8.0.1",
        "onchange": "^7.1.0",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "portfinder": "^1.0.13",
        "postcss-import": "^11.0.0",
        "postcss-loader": "^2.0.8",
        "postcss-url": "^7.2.1",
        "semver": "^5.3.0",
        "shelljs": "^0.7.6",
        "uglifyjs-webpack-plugin": "^1.1.1",
        "url-loader": "^0.5.8",
        "vue-loader": "^13.3.0",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.5.2",
        "webpack": "^3.12.0",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": ">=3.1.11",
        "webpack-merge": "^4.1.0"
    }
}
